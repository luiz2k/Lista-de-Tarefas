{
	"name": "lista-de-tarefas-back-end",
	"version": "1.0.0",
	"main": "dist/server.js",
	"engines": {
		"node": "20.14.0"
	},
	"type": "module",
	"scripts": {
		"build": "rm -rf dist && npx tsc",
		"start": "node --loader ts-node/esm dist/server.js",
		"dev": "tsx --watch src/server.ts",
		"test": "vitest",
		"typeorm": "typeorm-ts-node-commonjs",
		"docker:postgres": "docker compose up postgres",
		"migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/database/data-source ./src/database/migrations/migration",
		"migration:run": "npm run typeorm migration:run -- -d ./src/database/data-source",
		"migration:revert": "npm run typeorm migration:revert -- -d ./src/database/data-source"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-async-errors": "^3.1.1",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.5.0",
		"pg": "^8.12.0",
		"reflect-metadata": "^0.2.2",
		"swagger-ui-express": "^5.0.1",
		"typeorm": "^0.3.20",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@anatine/esbuild-decorators": "^0.2.19",
		"@babel/cli": "^7.24.8",
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.4",
		"@babel/preset-typescript": "^7.24.7",
		"@biomejs/biome": "^1.8.3",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^20.14.10",
		"@types/swagger-ui-express": "^4.1.6",
		"ts-node": "^10.9.2",
		"tsx": "^4.16.2",
		"typescript": "^5.5.3",
		"vitest": "^2.0.2"
	}
}
