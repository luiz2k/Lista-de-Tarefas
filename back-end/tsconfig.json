{
	"compilerOptions": {
		"target": "ES2022",
		"module": "node16",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
    "lib": ["ES2023"],
